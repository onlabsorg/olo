{{ title = "Standard steel profiles" }}

{{ HProfile: (b, h, s, t, r) -> {
    b = b,
    h = h,
    s = s,
    s = s,
    r = r,
    A = b*h - (b-s)*(h-2*t) + (4-pi)*r^2,
    Ix = b*h^3/12 - (b-s)*(h-2*t)^3/12 + 4 * ((1/3-pi/16)*r^4 + (h/2-t-r)*r^3/3 + (1-pi/4)*(h/2-t-r)^2*r^2),
    Iy = 2*t*b^3/12 + (h-2*t)*s^3/12 + 4 * ((1-5/16*pi)*r^4 + (5/6-pi/4)*r^3*s + (1/4-pi/16)*r^2*s^2),
    ix = (Ix/A)^0.5,
    iy = (Iy/A)^0.5
    } }}

{{ HE100A  = HProfile(0.100, 0.096, 0.0050, 0.0080, 0.012) }}



{{ PFS: (b, h, s, t, A) -> {
    b = b,
    h = h,
    s = s,
    s = s,
    A = A,
    r = ( (A - b*h + (b-s)*(h-2*t)) / (4-pi) )^0.5,
    Ix = b*h^3/12 - (b-s)*(h-2*t)^3/12 + 4 * ((1/3-pi/16)*r^4 + (h/2-t-r)*r^3/3 + (1-pi/4)*(h/2-t-r)^2*r^2),
    Iy = 2*t*b^3/12 + (h-2*t)*s^3/12 + 4 * ((1-5/16*pi)*r^4 + (5/6-pi/4)*r^3*s + (1/4-pi/16)*r^2*s^2),
    ix = (Ix/A)^0.5,
    iy = (Iy/A)^0.5
    } }}

