<% title = __path__ %>
<% path = import "/bin/path" %>
<% dirName = path.getDirName __path__ %>
<% items = (import dirName).items %>

<ul>
    <% {
        full: path -> dirName + path,
        anchor: path -> `<a href="${full path}.html">/${path}</a>`,
        list_item: path -> `<li>${anchor path}</li>`
    }.(map list_item (enum items)) %>
</ul>
